    "test-browser": "node test/server.js"
  },
  "dependencies": {
    "@derbyjs/tracks": "^1.0.0",
    "chokidar": "^3.5.3",
    "derby-parsing": "^0.8.0",
    "derby-templates": "^0.8.1",
    "html-util": "^0.2.3",
    "qs": "^6.11.0",
    "racer": "^1.0.3",
    "resolve": "^1.22.1",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "async": "^3.2.4",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "express": "^4.18.1",
    "jsdom": "^20.0.1",
    "jshint": "^2.13.5",
    "mocha": "^10.0.0"
  },
  "optionalDependencies": {},
  "bugs": {
    "url": "https://github.com/derbyjs/derby/issues"
